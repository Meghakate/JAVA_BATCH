<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.asthvinayak.model.Item" table="item_otm_xml_list">
		<id name="itemId" column="item_id" type="string" />
		<property name="itemName" column="item_name" type="string"
			length="10" />
		<property name="price" column="price" type="double" length="10" />
	</class>
</hibernate-mapping>